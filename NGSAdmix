#conversion of VCF to beagle file using ANGSD 
~/angsd/angsd -vcf-pl fst.above.0.5.recode.vcf -doMajorMinor 1 -out testGLs_fst.above.0.5 -doGlf 2

#command to run basic NGSAdmix data
./NGSadmix -likes inputBeagleFile.gz -K 2 -o outFileName -P 10

